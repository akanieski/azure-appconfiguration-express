    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "sinon": "^7.4.2",
    "ts-mocha": "^6.0.0",
    "ts-mock-imports": "^1.2.6",
    "typescript": "^3.6.3"
  },
